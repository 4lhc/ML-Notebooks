\begin<<562>>center<<562>>
    \adjustimage<<824>>max size=<<563>>0.9\linewidth<<563>><<564>>0.9\paperheight<<564>><<824>><<565>>Evaluation_Metrices_files/Evaluation_Metrices_7_2.png<<565>>
    \end<<566>>center<<566>>
